   "source": [
    "Now get average of the counts in the dictionary (minus one since count for each item will be at least one)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 65,
   "metadata": {
    "collapsed": true,
    "deletable": true,
    "editable": true
   },
   "outputs": [],
   "source": [
    "def avg_related_count(indict):\n",
    "    total = sum(indict.values())\n",
    "    if len(indict.values())==0:\n",
    "        return 0.\n",
    "    else:\n",
    "        return (total/(len(indict)) ) - 1."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 66,
   "metadata": {
    "collapsed": false,
    "deletable": true,
    "editable": true
   },
   "outputs": [],
   "source": [
    "df_save['related_score'] = df_save.related_counts.apply(avg_related_count)"
   ]
